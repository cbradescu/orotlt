{% extends 'OroUIBundle:actions:index.html.twig' %}
{% import 'OroDataGridBundle::macros.html.twig' as dataGrid %}
{% set pageTitle = '<%= bundle._name %>.<%= entity_name %>.entity_plural_label'|trans %}
{% set gridName = '<%= entity_name %>-grid' %}
{% block navButtons %}
    {% if true or resource_granted('<%= bundle_name %>_<%= entity_name %>_create') %}
        {{ UI.addButton({
            'path' : path('<%= bundle_name %>_<%= entity_name %>_create'),
            'entity_label': '<%= bundle._name %>.<%= entity_name %>.entity_label'|trans
        }) }}
    {% endif %}
{% endblock %}
